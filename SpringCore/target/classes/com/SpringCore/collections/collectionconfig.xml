<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns:p="http://www.springframework.org/schema/p"
xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">


<bean class="com.SpringCore.collections.Emp" name="employe1">
<property name="name" value="naran khadka"/>
<!-- using list  -->
<property name="phones">
<list>
<value>234223</value>
<value>0294920</value>
<value>344234</value>
</list>
</property>

<!-- Using the Set for the address -->
<property name="address" >
<set>
<value>kathmandu </value>
<value>kathmandu </value> <!-- this value can not print because it represent the duplicate values -->
<value>Ramechhao </value>
</set>
</property>

<!-- Using the Map for the courses  where we store the  course name and the duration -->
<property name="courses">
<map>
<entry key="java" value="2months"/>
<entry key="C" value="3months"/>
<entry key="python" value="5months"/>
</map>


</property>
<property name="props" >
<props>
<prop key="name">Naran</prop>
<prop key="password">Naran@123</prop>
</props>
</property>





</bean>
    

</beans>